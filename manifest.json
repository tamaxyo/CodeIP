{
  "name": "CodeIP",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["js/jquery.js", "js/background.js"],
    "persistence": false
  },
  "browser_action": {
    "default_icon": "icon.png", 
    "default_popup": "main.html"
  }, 
  "permissions": [
    "contextMenus", 
    "notifications",
    "tabs", 
    "webRequest",
    "webRequestBlocking", 
    "*://*/*"
  ]
}
